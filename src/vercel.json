{
  "version": 2,
  "name": "artvibe-platform",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_VERSION": "1.0.0",
    "REACT_APP_ENV": "production"
  }
}